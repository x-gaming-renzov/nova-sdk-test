		  "show_popup": {
			"type": "boolean",
			"description": "Whether to show welcome popup in lobby",
			"default": true
		  }
		}
	  },
	  "store-header": {
		"type": "Param",
		"keys": {
		  "store_title": {
			"type": "text",
			"description": "Store page title",
			"default": "Nova Store"
		  },
		  "store_subtitle": {
			"type": "text",
			"description": "Store page subtitle",
			"default": "Special offers and bundles just for you!"
		  }
		}
	  },
	  "store-products": {
		"type": "Param",
		"keys": {
		  "products": {
			"type": "text",
