		  },
		  "tagline": {
			"type": "text",
			"description": "Promotional tagline text",
			"default": "Join millions of players. Claim your welcome rewards and start your adventure!"
		  },
		  "username_placeholder": {
			"type": "text",
			"description": "Placeholder text for username input",
