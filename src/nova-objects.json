		  "welcome-offer-popup": true
		}
	  },
	  "lobby": {
		"description": "Main game lobby experience",
		"objects": {
		  "main-lobby": true
		}
	  },
	  "store": {
		"description": "In-game store and monetization",
		"objects": {
		  "store-header": true,
		  "store-products": true
