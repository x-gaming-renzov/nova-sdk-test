			"default": "[{\"id\":\"starter_pack\",\"name\":\"Starter Pack\",\"description\":\"Perfect for new heroes\",\"discount\":20,\"tag\":\"POPULAR\"},{\"id\":\"gem_bundle\",\"name\":\"Gem Bundle\",\"description\":\"1000 premium gems\",\"discount\":0,\"tag\":\"\"},{\"id\":\"hero_boost\",\"name\":\"Hero Boost\",\"description\":\"Double XP for 24 hours\",\"discount\":15,\"tag\":\"LIMITED\"}]"
